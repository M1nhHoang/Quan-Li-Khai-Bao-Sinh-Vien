<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="quan-li-khai-bao-main">
        <dropUniqueConstraint constraintName="users_email_unique" tableName="users"/>
    </changeSet>
    <changeSet id="2" author="quan-li-khai-bao-main">
        <dropTable cascadeConstraints="true" tableName="failed_jobs"/>
    </changeSet>
    <changeSet id="3" author="quan-li-khai-bao-main">
        <dropTable cascadeConstraints="true" tableName="migrations"/>
    </changeSet>
    <changeSet id="4" author="quan-li-khai-bao-main">
        <dropTable cascadeConstraints="true" tableName="password_resets"/>
    </changeSet>
    <changeSet id="5" author="quan-li-khai-bao-main">
        <dropTable cascadeConstraints="true" tableName="users"/>
    </changeSet>
</databaseChangeLog>